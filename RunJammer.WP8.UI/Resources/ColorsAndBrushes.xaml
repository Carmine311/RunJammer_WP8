	<Color x:Key="1RunRatingColor">#FF298AEA</Color>
	<SolidColorBrush x:Key="1RunRatingBrush"
					 Color="{StaticResource 1RunRatingColor}" />
	<Color x:Key="2RunRatingColor">#FF6AB66A</Color>
	<SolidColorBrush x:Key="2RunRatingBrush"
					 Color="{StaticResource 2RunRatingColor}" />
</ResourceDictionary>